{
  "name": "jenkins-doc",
  "version": "1.8.0",
  "displayName": "Jenkins Doc Extendable",
  "description": "Provides Jenkins documentation and autocompletion on Jenkinsfile and Groovy files",
  "publisher": "Tiegris",
  "license": "GPL-3.0",
  "icon": "assets/logo_128.png",
  "galleryBanner": {
    "color": "#241f20",
    "theme": "dark"
  },
  "author": {
    "name": "Tieger Bal√°zs"
  },
  "homepage": "https://github.com/Tiegris/JenkinsDocExtension/blob/develop/README.md",
  "bugs": {
    "url": "https://github.com/Tiegris/JenkinsDocExtension/issues"
  },
  "repository": {
    "url": "https://github.com/Tiegris/JenkinsDocExtension"
  },
  "keywords": [
    "jenkins",
    "groovy",
    "doc",
    "jenkinsfile",
    "automation"
  ],
  "engines": {
    "vscode": "^1.54.0"
  },
  "categories": [
    "Programming Languages",
    "Keymaps",
    "Snippets",
    "Other"
  ],
  "activationEvents": [
    "onLanguage:groovy"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [],
    "configuration": {
      "title": "Jenkins Doc",
      "properties": {
        "jenkins-doc.dataExtensionPath": {
          "type": "string",
          "default": "",
          "description": "Add your own jenkins data."
        }
      }
    }
  },
  "qna": false,
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "publish": "vsce publish",
    "package": "vsce package",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "scraper": "nodemon --watch 'doc-scraper/**/*.ts' --exec npm run scraper-exec",
    "scraper-exec": "npx ts-node doc-scraper/index.ts"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/cheerio": "^0.22.24",
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.54.0",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "axios": "^0.21.1",
    "cheerio": "^1.0.0-rc.5",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^8.1.0",
    "glob": "^7.1.6",
    "mocha": "^8.2.1",
    "nodemon": "^2.0.7",
    "prettier": "2.2.1",
    "rxjs": "^6.6.6",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3",
    "vscode-test": "^1.5.0"
  }
}
